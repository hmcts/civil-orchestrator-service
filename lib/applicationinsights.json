{
  "connectionString": "${file:/mnt/secrets/civil/app-insights-connection-string}",
  "role": {
    "name": "civil-orchestrator-service"
  },
  "preview": {
    "sampling" : {
      "overrides" : [
        {
          "telemetryType" : "request",
          "attributes" : [
            {
              "key" : "http.url",
              "value" : "https?://[^/]+/health.*",
              "matchType" : "regexp"
            }
          ],
          "percentage" : 1
        }
      ]
    }
  }
}
